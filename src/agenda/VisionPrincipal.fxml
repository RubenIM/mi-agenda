<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="BorderPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="agenda.VisionPrincipalController">
  <children>
    <Button fx:id="btnAgenda" layoutX="81.0" layoutY="123.0" mnemonicParsing="false" onAction="#muestraVistasPersona" text="">
      <graphic>
        <ImageView id="imagenAgenda" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@util/imagenes/notebook.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <Button fx:id="btnNotas" layoutX="363.0" layoutY="123.0" mnemonicParsing="false" onAction="#muestraVistasAnotaciones" text="">
      <graphic>
        <ImageView id="imagenNotas" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@util/imagenes/adressnotebook.png" />
          </image>
        </ImageView>
      </graphic>
    </Button>
  </children>
</AnchorPane>
